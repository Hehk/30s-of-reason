{
  "name": "server",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    },
    "snippets"
  ],
  "package-specs": {
    "module": "commonjs"
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-algolia",
    "bs-express",
    "bs-graphql",
    "bs-graphql-tools",
    "bs-apollo-server-express",
    "reason-react",
    "reason-apollo",
    "bs-nice",
    "bs-nice-components"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "reason": {
    "react-jsx": 2
  },
  "ppx-flags": [
    "graphql_ppx/ppx"
  ],
  "namespace": true,
  "refmt": 3
}
